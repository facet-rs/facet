// Benchmark definitions for code generator
// Each benchmark automatically gets divan + gungraun versions for all 5 targets

benchmark name="simple_struct" type="SimpleRecord" category="micro" {
    json "{\"id\": 42, \"name\": \"test\", \"active\": true}"
}

// Struct key length variations - tests dispatch strategy selection
// Short keys (all ≤5 chars): uses Inline dispatch (pattern matching)
benchmark name="short_keys" type="ShortKeys" category="micro" {
    json "{\"id\": 42, \"name\": \"test\", \"age\": 30, \"ok\": true}"
}

// Medium keys (some 6-10 chars): uses Linear dispatch
benchmark name="medium_keys" type="MediumKeys" category="micro" {
    json "{\"userId\": 42, \"userName\": \"test\", \"isActive\": true, \"counter\": 100}"
}

// Long keys (>10 chars): uses PrefixSwitch dispatch
benchmark name="long_keys" type="LongKeys" category="micro" {
    json "{\"userIdentifier\": 42, \"displayNameValue\": \"test\", \"isCurrentlyActive\": true}"
}

benchmark name="single_nested_struct" type="Outer" category="micro" {
    json "{\"id\": 42, \"inner\": {\"x\": 10, \"y\": 20}, \"name\": \"test\"}"
}

// Nesting level variations - tests recursive compilation overhead
benchmark name="nest_1" type="Nest1" category="micro" {
    json "{\"id\": 1, \"val\": \"a\"}"
}

benchmark name="nest_2" type="Nest2" category="micro" {
    json "{\"id\": 2, \"inner\": {\"id\": 1, \"val\": \"a\"}}"
}

benchmark name="nest_3" type="Nest3" category="micro" {
    json "{\"id\": 3, \"inner\": {\"id\": 2, \"inner\": {\"id\": 1, \"val\": \"a\"}}}"
}

benchmark name="simple_with_options" type="WithOptions" category="micro" {
    json "{\"id\": 42, \"maybe_count\": 123, \"maybe_flag\": null, \"maybe_value\": 2.5}"
}

// Type definitions
type_def name="SimpleRecord" {
