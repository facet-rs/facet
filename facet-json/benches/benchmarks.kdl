}

// Medium keys: 6-10 char field names - uses Linear dispatch
type_def name="MediumKeys" {
    code """
#[derive(Debug, PartialEq, Facet, serde::Serialize, serde::Deserialize, Clone)]
struct MediumKeys {
    #[facet(rename = "userId")]
    #[serde(rename = "userId")]
    user_id: u64,         // 6 chars
    #[facet(rename = "userName")]
    #[serde(rename = "userName")]
    user_name: String,    // 8 chars
    #[facet(rename = "isActive")]
    #[serde(rename = "isActive")]
    is_active: bool,      // 8 chars
    counter: i64,         // 7 chars
}
"""
}

// Long keys: >10 char field names - uses PrefixSwitch dispatch (if >10 fields)
type_def name="LongKeys" {
    code """
#[derive(Debug, PartialEq, Facet, serde::Serialize, serde::Deserialize, Clone)]
